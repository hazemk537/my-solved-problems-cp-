vector<ll>data(1000000);
ordered_set os;
ll mod=1000000007;
int main()
{
    start();

int test; cin>>test;
while(test--)
{

ll x,y,n; cin>>x>>y>>n;
ll mod=n%x;


if(mod==y) cout<<n<<endl;
else if(mod>y) cout<<n-(mod-y)<<endl;
 else  cout<<n-(mod+(x-y))<<endl;
}
